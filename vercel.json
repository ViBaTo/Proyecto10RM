{
  "version": 2,
  "builds": [
    {
      "src": "back/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "front/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "back/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "front/$1"
    }
  ]
}
