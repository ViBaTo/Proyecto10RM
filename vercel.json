{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd front && npm install && npm run build",
  "outputDirectory": "front/dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  }
}
